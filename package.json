{
  "name": "tanstack-start-tutorial-yt",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "format": "prettier",
    "check": "prettier --write . && eslint --fix",
    "post-cta-init": "npx create-db@latest --user-agent tanstack/tsrouter",
